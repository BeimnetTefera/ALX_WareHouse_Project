CREATE TABLE bronze.pw_learner_activity(
	  first_name NVARCHAR(50),
    last_name NVARCHAR(50),
    email NVARCHAR(100),
    course_name NVARCHAR(150),
    assignment_name NVARCHAR(150),
    is_assignment_accessed NVARCHAR(3),
    is_assignment_submitted NVARCHAR(3),
    is_assignment_passed NVARCHAR(3),
    contact_phone  NVARCHAR(20),
    age INT,
    age_range NVARCHAR(5),
    country_of_residence NVARCHAR(50),
    category_country_of_residences NVARCHAR(50),
    city_of_residence NVARCHAR(50),
    day_of_activation INT,
    cohort_name NVARCHAR(50),
    class_name NVARCHAR(50),
    location_program_cohort NVARCHAR(50),
    assignment_type NVARCHAR(50),
    assignment_score FLOAT,
    learner_enrolled_into_lms NVARCHAR(3),
    has_logged_into_lms NVARCHAR(3),
    has_logged_into_eHub NVARCHAR(3),
    has_shown_up_in_circle NVARCHAR(3),
    has_shown_up_in_circle_during_the_last_two_days NVARCHAR(3),
    is_engaged_in_circle NVARCHAR(3),
    is_engaged_in_circle_during_the_last_two_days NVARCHAR(3),
    enrollment_program_activated NVARCHAR(3),
    enrollment_in_previous_withdrawn_class NVARCHAR(3),
    enrollment_course_activated NVARCHAR(3),
    is_program_withdrawn  NVARCHAR(3),
    program_withdrawal_reason NVARCHAR(254),
    program_withdrawn_date DATETIME,
    is_course_defeerd NVARCHAR(3),
    course_deferred_date DATETIME,
    on_track_for_on_time_graduation NVARCHAR(3),
    on_track_for_delayed_graduation NVARCHAR(3),
	  is_assignment_resubmitted NVARCHAR(3),
    iassignment_resubmitted_and_not_corrected_yet NVARCHAR(3),
    is_assignment_graded NVARCHAR(3),
    is_assignment_corrected_by_an_external_grader NVARCHAR(3),
    corrected_external_grader_name NVARCHAR(50),
    corrected_external_grader_email NVARCHAR(100),
    is_on_track NVARCHAR(3),
    no_of_assignment_submissions INT,
    submission_link NVARCHAR(250),
    no_of_assignmnets INT,
    no_of_submissions INT,
    no_of_assignments_passed INT,
    no_of_milestone INT,
    no_of_milestone_submissions INT,
    no_of_milestones_passed INT,
    no_of_quizzes INT,
    no_of_quizzes_submitted INT,
    no_of_quizzes_passed INT,
    no_of_tests INT,
    no_of_tests_submissions INT,
    no_of_tests_passed INT,
    lms_overall_score FLOAT,
    is_lms_overall_score_higher_than_score_threshold NVARCHAR(3),
    lms_course_status NVARCHAR(50),
    ehub_course_status NVARCHAR(50),
    ehub_class_status NVARCHAR(50),
    is_course_paused NVARCHAR(3),
    course_leave_of_absence_reason NVARCHAR (254)
);
