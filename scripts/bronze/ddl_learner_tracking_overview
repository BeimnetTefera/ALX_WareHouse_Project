IF OBJECT_ID('bronze.pw_learner_tracking_overview', 'U') IS NOT NULL
    DROP TABLE bronze.pw_learner_tracking_overview;

CREATE TABLE bronze.pw_learner_tracking_overview(
    first_name NVARCHAR(50),
    last_name NVARCHAR(50),
    email NVARCHAR(100),
    course_name NVARCHAR(150),
    no_of_submissions INT,
    no_assignments_passed INT,
    lms_overall_score FLOAT,
    hubspot_id NVARCHAR(100),
    contact_phone NVARCHAR(20),
    age INT,
    age_range NVARCHAR(20),
    country_of_residence NVARCHAR(50),
    category_country_of_residence NVARCHAR(50),
    city_of_residence NVARCHAR(50),
    cohort_name NVARCHAR(50),
    location_program_cohort NVARCHAR(100),
    learner_enrolled_into_lms NVARCHAR(3),
    has_logged_into_lms NVARCHAR(3),
    has_logged_into_ehub NVARCHAR(3),
    has_shown_up_in_circle NVARCHAR(3),
    has_shown_up_in_circle_last_two_days NVARCHAR(3),
    is_engaged_in_circle NVARCHAR(3),
    is_engaged_in_circle_last_two_days NVARCHAR(3),
    enrollment_program_activated NVARCHAR(3),
    class_name NVARCHAR(50),
    enrollment_course_activated NVARCHAR(3),
    is_program_withdrawn NVARCHAR(3),
    program_withdrawal_reason NVARCHAR(254),
    program_withdrawn_date DATETIME,
    is_course_deferred NVARCHAR(3),
    course_deferred_date DATETIME,
    on_track_for_on_time_graduation NVARCHAR(3),
    on_track_for_delayed_graduation NVARCHAR(3),
    no_of_assignments INT,
    no_of_milestone INT,
    no_of_milestone_submissions INT,
    no_of_milestone_passed INT,
    no_of_practice_quiz INT,
    no_of_practice_quiz_submissions INT,
    no_of_practice_quiz_passed INT,
    no_of_test INT,
    no_of_test_submissions INT,
    no_of_test_passed INT,
    assignments_accessed INT,
    assignments_submitted INT,
    assignments_missing INT,
    assignments_passed INT,
    assignments_submitted_failed INT,
    learner_hub_city NVARCHAR(50),
    first_check_in DATETIME,
    most_recent_check_in DATETIME,
    total_check_in_any_hub INT,
    no_of_check_in_most_visited_hub INT,
    current_month_to_day_check_in INT,
    check_ins_last_30_days INT,
    meets_attendance_criteria NVARCHAR(3),
    most_visited_hub NVARCHAR(50),
    is_lms_overall_score_higher_than_threshold NVARCHAR(3),
    lms_course_status NVARCHAR(50),
    ehub_course_status NVARCHAR(50),
    ehub_class_status NVARCHAR(50),
    is_course_paused NVARCHAR(3),
    course_leave_of_absence_reason NVARCHAR(254),
    nile_cohort_name NVARCHAR(50)
);
